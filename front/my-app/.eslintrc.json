{
    "env": {
    "browser": true,
    "es2021": true
    },
    "extends": ["react-app", "prettier/prettier"],
    "parserOptions": {
    "ecmaFeatures": {
    "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
    },
    "plugins": [
    "react",
    "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
      }
   }
